<?xml version="1.0"?>
<Container>
  <Name>qbittorrent</Name>
  <Repository>80x86/qbittorrent:4.3.0-focal-20200925-amd64</Repository>
  <Registry>https://hub.docker.com/r/80x86/qbittorrent</Registry>
  <Network>host</Network>
  <Privileged>false</Privileged>
  <Support>https://www.52ljt.win</Support>
  <Project>https://hub.docker.com/r/80x86/qbittorrent</Project>
  <Overview>qbittorrent</Overview>
  <Description>
    使用荒野无灯大神的qbittorrent镜像qbittorrent:4.3.0-focal-20200925-amd64
  </Description>
  <Category>Cloud: Network:Web Status:Stable</Category>
  <WebUI>http://[IP]:[PORT:8082]</WebUI>
  <Icon>https://github.com/louis000008/unraid/blob/master/qbittorrent.png</Icon>
  <ExtraParams>--mount type=tmpfs,destination=/tmp</ExtraParams>
  <PostArgs></PostArgs>
  <CPUset/>
  <DateInstalled></DateInstalled>
  <DonateText/>
  <DonateLink/>
  <Description></Description>
  <Data>
    <Volume>
      <HostDir>/mnt/user/appdata/qbittorrent/config</HostDir>
      <ContainerDir>/config</ContainerDir>
      <Mode>rw</Mode>
    </Volume>
    <Volume>
      <HostDir>/mnt/user/appdata/qbittorrent/data</HostDir>
      <ContainerDir>/data</ContainerDir>
      <Mode>rw</Mode>
    </Volume>
    <Volume>
      <HostDir>/mnt/user</HostDir>
      <ContainerDir>/downloads</ContainerDir>
      <Mode>rw</Mode>
    </Volume>
  </Data>
  <Environment>
    <Variable>
      <Value>1000</Value>
      <Name>PUID</Name>
      <Mode/>
    </Variable>
    <Variable>
      <Value>100</Value>
      <Name>PGID</Name>
      <Mode/>
    </Variable>
    <Variable>
      <Value>8999</Value>
      <Name>BT_PORT</Name>
      <Mode/>
    </Variable>
    <Variable>
      <Value>8082</Value>
      <Name>WEB_PORT</Name>
      <Mode/>
    </Variable>
  </Environment>
</Container>
